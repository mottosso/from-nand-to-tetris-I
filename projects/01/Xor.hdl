/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    	Not(in=a, out=notA);
    	Not(in=b, out=notB);
    	And(a=notA, b=b, out=andA);
    	And(a=a, b=notB, out=andB);
    	Or(a=andA, b=andB, out=out);
}